{
  "name": "lango-client",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run zip",
    "zip": "node ./scripts/compression/index.mjs",
    "storybook": "storybook dev --config-dir ./configs/storybook",
    "storybook:build": "storybook build --config-dir ./configs/storybook",
    "lint:ts": "eslint . --ext .js,.ts,.tsx",
    "lint:ts:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
    "format": "prettier --write \"*.{ts,Ð¼tsx}\"",
    "test:unit": "vitest run --config configs/vitest/vitest.config.ts",
    "test:reports": "npx vite preview --outDir reports",
    "analyzer": "npx vite-bundle-analyzer --port=8889",
    "lint-staged": "lint-staged",
    "generate:slice": "node ./scripts/createSlice/index.js",
    "preview": "vite preview",
    "prepare": "husky",
    "postinstall": "node ./scripts/cache/clear-chache.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@reduxjs/toolkit": "^2.10.1",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.68.0",
    "react-intl": "^7.1.14",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.5",
    "recharts": "^3.6.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@chromatic-com/storybook": "^4.1.2",
    "@meow-double/eslint": "^2.1.1",
    "@meow-double/stylelint": "^1.0.1",
    "@storybook/addon-a11y": "^10.0.6",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/addon-onboarding": "^10.0.6",
    "@storybook/addon-themes": "^10.1.2",
    "@storybook/react": "^10.0.6",
    "@storybook/react-vite": "^10.0.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "msw": "^2.12.3",
    "prettier": "^3.6.2",
    "sass": "^1.93.3",
    "storybook": "^10.0.6",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-circular-dependency": "^0.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.13"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:ts",
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public",
      "public/scripts"
    ]
  }
}
