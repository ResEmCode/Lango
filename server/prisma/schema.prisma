generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id    Int    @id @default(autoincrement())
  email String
  login String

  role AccountRole

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum AccountRole {
  STANDART
  PREMIUM
  PLATINUM
  ADMIN
  SUPERUSER
}
