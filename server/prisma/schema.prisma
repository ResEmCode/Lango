generator client {
  provider = "prisma-client"
  output   = "./generated/prisma"
}

//test pipeline for server

datasource db {
  provider = "postgresql"
}

model User {
  id           String @id @default(uuid()) @db.Uuid
  avatar       String
  email        String
  username     String
  hashPassword String

  role AccountRole

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum AccountRole {
  STANDART
  PREMIUM
  PLATINUM
  ADMIN
  SUPERUSER
}
